@{
    var data = ViewBag.StudentData;
}

<table>
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Father Name</th>
    </tr>



    @foreach(var item in data)
    {
        <tr>
        <td>@item.Id</td>
        <td>@item.Name</td>
        <td>@item.FatherName</td>
        </tr>
    }
</table>
